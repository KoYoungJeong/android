<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory
        android:key="setting_push"
        android:title="알림 설정">
        <CheckBoxPreference
            android:key="setting_push_auto_alarm"
            android:title="자동 알림"
            android:summaryOff="푸쉬 알림을 받지 않습니다."
            android:summaryOn="푸쉬 알림을 받습니다."
            android:defaultValue="true"/>
    </PreferenceCategory>
    <PreferenceCategory
        android:key="setting_general"
        android:title="일반">
        <Preference
            android:key="setting_logout"
            android:title="로그아웃"
            android:selectable="true" />
    </PreferenceCategory>

</PreferenceScreen>